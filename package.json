    "build-ts": "tsc",
    "watch-ts": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "^8.10.20",
    "cross-env": "^5.2.0",
    "gh-pages": "^1.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.1.7",
    "@koa/cors": "^2.2.1",
    "boardgame.io": "^0.24.0",
    "colyseus": "^0.9.12",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "jws": "^3.1.5",
    "koa": "^2.5.1",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^3.3.2",
    "koa-mount": "^3.0.0",
    "redux": "^3.7.2",
    "request": "^2.87.0",
